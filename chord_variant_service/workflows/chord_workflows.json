{
  "ingestion": {
    "vcf_gz": {
      "name": "TBI Generation",
      "description": "Given a bgzip-compressed VCF, this ingestion workflow will generate a .tbi file for the data.",
      "data_type": "variant",
      "file": "vcf_gz.wdl",
      "inputs": [
        {
          "id": "vcf_gz_files",
          "type": "file[]",
          "extensions": [".vcf.gz"]
        },
        {
          "id": "assembly_id",
          "type": "enum",
          "values": ["GRCh38", "GRCh37", "NCBI36", "Other"],
          "default": "GRCh38"
        }
      ],
      "outputs": [
        {
          "id": "vcf_gz_files",
          "type": "file[]",
          "map_from_input": "vcf_gz_files",
          "value": "{}"
        },
        {
          "id": "tbi_files",
          "type": "file[]",
          "map_from_input": "vcf_gz_files",
          "value": "{}.tbi"
        }
      ]
    },
    "vcf": {
      "name": "VCF Compression and TBI Generation",
      "description": "Given a VCF, this ingestion workflow will generate a .vcf.gz and a .tbi file for the data.",
      "data_type": "variant",
      "file": "vcf.wdl",
      "inputs": [
        {
          "type": "file[]",
          "id": "vcf_files",
          "extensions": [".vcf"]
        },
        {
          "type": "enum",
          "id": "assembly_id",
          "values": ["GRCh38", "GRCh37", "NCBI36", "Other"],
          "default": "GRCh38"
        }
      ],
      "outputs": [
        {
          "id": "vcf_gz_files",
          "type": "file[]",
          "map_from_input": "vcf_files",
          "value": "{}.gz"
        },
        {
          "id": "tbi_files",
          "type": "file[]",
          "map_from_input": "vcf_files",
          "value": "{}.gz.tbi"
        }
      ]
    }
  },
  "analysis": {}
}
